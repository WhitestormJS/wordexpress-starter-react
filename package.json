{
  "name": "univeral-hmr-ssr-react-redux-dokku",
  "version": "1.0.0",
  "description": "Univeral JS application utilizing Hot Module Reloading, Server Side Rendering, Code Splitting with React, Redux, React Router 4 and a heroku like deploy setup with Dokku.",
  "main": "index.js",
  "scripts": {
    "development": "NODE_PATH=./src NODE_ENV=development node ./src/server/server.babel.js",
    "production": "NODE_PATH=./src NODE_ENV=production node ./src/server/server.babel.js",
    "build": "rimraf ./build && npm run build:client && npm run build:server",
    "build:server": "NODE_ENV=production webpack --config ./webpack/server.babel.js",
    "build:client": "NODE_ENV=production webpack --config ./webpack/client.babel.js"
  },
  "keywords": [
    "react",
    "redux",
    "hot-module-reloading",
    "server-side-rendering",
    "hmr",
    "ssr",
    "code-splitting",
    "webpack"
  ],
  "author": "Alexander J Ray <alexjamesray@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.7",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.0.4",
    "apollo-link": "^1.0.7",
    "apollo-link-http": "^1.3.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-css-modules-transform": "^1.3.1",
    "babel-plugin-graphql-tag": "^1.3.0",
    "babel-plugin-inline-import-graphql-ast": "^2.0.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "express": "^4.15.2",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "gsap": "^1.20.3",
    "history": "^4.6.1",
    "http-proxy-middleware": "^0.17.4",
    "immutable": "^3.8.1",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "react": "^15.4.2",
    "react-apollo": "^2.0.4",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.4",
    "redux": "^3.6.0",
    "sass-loader": "^6.0.6",
    "transform-runtime": "^0.0.0"
  }
}
